{
  "name": "@derivable/fetcher-univ2",
  "description": "Price fetcher for Uniswap V2",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "patch": "node patch.js",
    "clean": "rimraf ./build/ ./cache/ ./artifacts/",
    "compile": "hardhat compile",
    "build": "hardhat compile && cp artifacts/contracts/FetcherV2.sol/FetcherV2.json build/",
    "getPrice:ganache": "npx hardhat run scripts/deploy.ganache.ts --network ganacheservice && npx hardhat run scripts/swapMonkey.ts --network ganacheservice && npx hardhat run scripts/getPrice.ts --network ganacheservice",
    "test": "npx hardhat --network ganacheservice test",
    "coverage": "hardhat coverage",
    "lint": "hardhat check",
    "deploy": "npx hardhat run scripts/sf.deploy.ts",
    "deploy:ganache": "npx hardhat run scripts/deploy.ganache.ts --network ganacheservice"
  },
  "dependencies": {
    "@keydonix/uniswap-oracle-contracts": "^1.0.6",
    "@keydonix/uniswap-oracle-sdk": "^1.0.7",
    "@keydonix/uniswap-oracle-sdk-adapter": "^1.0.7",
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@openzeppelin/contracts": "3",
    "dotenv": "^16.0.1",
    "ethers": "^5.5.4",
    "hardhat": "^2.17.1",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.9"
  },
  "devDependencies": {
    "@derivable/utr": "https://github.com/derivable-labs/utr.git",
    "@ganache/console.log": "^0.4.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@peculiar/webcrypto": "1.1.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "14.0.5",
    "@types/node-fetch": "^2.6.4",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@zoltu/ethereum-crypto": "^2.3.0",
    "@zoltu/ethereum-fetch-json-rpc": "^13.1.4",
    "@zoltu/rlp-encoder": "2.0.0",
    "@zoltu/solidity-typescript-generator-fetch-dependencies": "5.0.2",
    "canonical-weth": "^1.4.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.4.4",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.6",
    "solidity-coverage": "^0.8.5",
    "solidity-rlp": "https://github.com/hamdiallam/Solidity-RLP.git",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "web3": "^4.1.1"
  }
}
